<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>trimmy.ico</ApplicationIcon>
    <AssemblyName>WinTrimmy</AssemblyName>
    <RootNamespace>WinTrimmy</RootNamespace>
    <Version>1.0.0</Version>
    <Authors>WinTrimmy</Authors>
    <Description>Automatically flattens multi-line shell commands copied to clipboard</Description>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

</Project>
